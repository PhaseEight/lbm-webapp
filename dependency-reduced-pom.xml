    <org.springframework.webflow-version>2.4.0.M1</org.springframework.webflow-version>
    <org.hibernate.hibernate-validator-version>4.3.1.Final</org.hibernate.hibernate-validator-version>
    <joda-time-version>2.2</joda-time-version>
    <javax.servlet.servlet-api-version>2.5</javax.servlet.servlet-api-version>
    <com.h2database.h2-version>1.3.173</com.h2database.h2-version>
    <java-target-version>1.7</java-target-version>
    <org.apache.commons.commons-io-version>1.3.2</org.apache.commons.commons-io-version>
    <primefaces.themes-version>1.0.10</primefaces.themes-version>
    <m2eclipse.wtp.contextRoot>/lbm</m2eclipse.wtp.contextRoot>
    <org.slf4j-version>1.6.6</org.slf4j-version>
    <org.jadira.usertype.usertype.core-version>3.0.0.GA</org.jadira.usertype.usertype.core-version>
    <org.aspectj-version>1.6.10</org.aspectj-version>
    <commons-dbcp-version>1.2.2</commons-dbcp-version>
    <com.google.guava-version>14.0.1</com.google.guava-version>
    <org.cloudfoundry.cloudfoundry-runtime-version>0.8.4</org.cloudfoundry.cloudfoundry-runtime-version>
    <maven-eclipse-plugin-version>2.9</maven-eclipse-plugin-version>
    <com.sun.faces-version>2.1.13</com.sun.faces-version>
    <commons-fileupload.commons-fileupload-version>1.3</commons-fileupload.commons-fileupload-version>
    <primefaces-version>4.0-SNAPSHOT</primefaces-version>
