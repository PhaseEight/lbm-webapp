<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.logbookmanager.domain.model.logbook.LogbookUserLogbookEntry"
		table="logbookuser_logbook_entry">
		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="increment">
			</generator>
		</id>

		<version name="version" column="version" type="java.lang.Long" />

		<property name="active" type="java.lang.Boolean" update="true">
			<column name="ACTIVE" not-null="true" unique="false" default="true" />
		</property>

		<property name="lastUpdateTimeStamp" type="java.sql.Timestamp">
			<column name="LAST_UPDATE_DATE" length="10" not-null="true"
				unique="false" default="CURRENT_TIMESTAMP" />
		</property>

		<many-to-one name="logbookUserLogbook"
			class="com.logbookmanager.domain.model.logbook.LogbookUserLogbook">
			<column name="LOGBOOKUSER_LOGBOOK_ID" not-null="true" />
		</many-to-one>
		<many-to-one name="logbookUser"
			class="com.logbookmanager.domain.model.logbook.LogbookUser">
			<column name="LOGBOOKUSER_ID" not-null="true" />
		</many-to-one>
		<property name="entryNumber" type="integer">
			<column name="ENTRY_NUMBER" />
		</property>

		<set name="entryAttributes" inverse="true" cascade="all-delete-orphan">
			<key>
				<column name="LOGBOOKUSER_LOGBOOK_ENTRY_ID" not-null="true" />
			</key>
			<one-to-many class="com.logbookmanager.domain.model.logbook.EntryAttribute" />
		</set>
	</class>
</hibernate-mapping>
